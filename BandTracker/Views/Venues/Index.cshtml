@{
  Layout = "_Layout";
}


<h1>Band Traker</h1>
<hr />
<h2>Venues:</h2>
@if (Model.Count != 0)
{
  <ul>
    @foreach (var venue in Model)
    {
      <li><a href="/venues/@venue.GetId()">@venue.GetVenueName()</a></li>
    }
  </ul>
}
<h4><a href="/venues/new">Add New Venue</a></h4>
<h4><a href="/">Return home</a></h4>
